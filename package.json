{
  "name": "toki-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "dev": "npm-run-all --parallel docs:dev repo:dev",
    "build": "lerna run build --scope=@toki-website/next-app",
    "export": "lerna run export --scope=@toki-website/next-app",
    "eslint:check": "eslint --ext ts,tsx .",
    "eslint:fix": "eslint --ext ts,tsx --fix .",
    "repo:dev": "lerna run dev --parallel",
    "docs:dev": "start-storybook -p 6006",
    "docs:build": "build-storybook"
  },
  "workspaces": [
    "packages/*",
    "docs"
  ],
