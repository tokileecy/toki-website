    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-testing-library": "^4.6.0",
    "glslify-loader": "^2.0.0",
    "lerna": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "raw-loader": "^4.0.2",
    "stylelint-processor-styled-components": "^1.10.0",
    "typescript": "^4.5.5",
    "webpack": "^5.47.1"
